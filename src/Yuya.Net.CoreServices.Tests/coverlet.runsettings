<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <Format>json,cobertura,lcov,opencover</Format>
          <Exclude>[Yuya.Net.CoreServices.Tests?]*,</Exclude>          <!-- [Assembly-Filter]Type-Filter <Exclude>[coverlet.*.tests?]*,[*]Coverlet.Core*</Exclude>-->
          <Include>[Yuya.Net.CoreServices.*]*,[*]Yuya.Net.CoreServices*</Include>          <!-- [Assembly-Filter]Type-Filter  <Include>[coverlet.*]*,[*]Coverlet.Core*</Include>-->
          <ExcludeByAttribute>Obsolete,GeneratedCodeAttribute,CompilerGeneratedAttribute</ExcludeByAttribute>
          <!-- <ExcludeByFile>**/dir1/class1.cs,**/dir2/*.cs,**/dir3/**/*.cs,</ExcludeByFile>-->          <!-- Globbing filter -->
          <IncludeDirectory>../Yuya.Net.CoreServices/,</IncludeDirectory>
          <SingleHit>false</SingleHit>
          <UseSourceLink>true</UseSourceLink>
          <IncludeTestAssembly>true</IncludeTestAssembly>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>